{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**", "config/**", "routes/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/index.ts"
    },
    {
      "src": "/swagger.json",
      "dest": "src/index.ts"
    },
    {
      "src": "/api-docs",
      "dest": "src/index.ts"
    }
  ]
}
